{
  "version": 2,
  "builds": [
    {
      "src": "simple_pipeline_api.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "simple_pipeline_api.py"
    },
    {
      "src": "/(.*)",
      "dest": "simple_pipeline_api.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./",
    "FLASK_ENV": "production"
  }
}
